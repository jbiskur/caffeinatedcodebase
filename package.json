{
  "name": "caffeinatedcodebase",
  "version": "1.0.0",
  "description": "The website for the caffeinated codebase blog site built with Flowcore IDD",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:image": "chmod +x ./scripts/build-image.sh && ./scripts/build-image.sh",
    "db:push": "env-cmd -x drizzle-kit push:pg --schema=src/database/schemas.ts --driver pg --connectionString=\\$POSTGRES_URL",
    "dev": "next dev",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "start": "next start",
    "docker:db": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:app": "docker compose --env-file .container.env up",
    "flowcore:dev": "flowcore data-core apply -f flowcore.yaml -f flowcore.local.yaml",
    "flowcore:prod": "flowcore apply -f flowcore.yaml",
    "local:stream": "env-cmd -x flowcore scenario local -f flowcore.yaml -f flowcore.local.yaml -f flowcore.local.development.yaml -s now -e http://localhost:3000/api/transform -H \"X-Secret: \\$TRANSFORMER_SECRET\"",
    "local:stream:backlog": "env-cmd -x flowcore scenario local -f flowcore.yaml -f flowcore.local.yaml -f flowcore.local.development.yaml -e http://localhost:3000/api/transform -H \"X-Secret: \\$TRANSFORMER_SECRET\"",
    "translate:dev": "chokidar 'translations/*.json' -c 'yarn translate'",
    "translate": "node translations/generate-typings.js",
    "prepare": "biome check --write"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.9.0",
    "@ducanh2912/next-pwa": "^10.2.5",
    "@flowcore/sdk-nextjs-backend": "^1.2.0",
